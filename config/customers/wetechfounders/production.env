# Customer Production Environment Variables
# Template for wetechfounders production environment

# Customer Information
CUSTOMER_NAME=wetechfounders
CUSTOMER_DOMAIN=wetechfounders.com
ENVIRONMENT=production

# Domain Configuration
DOMAIN=wetechfounders.com
API_DOMAIN=api.wetechfounders.com
AUTH_DOMAIN=auth.wetechfounders.com

# Database Configuration
DATABASE_URL=postgresql://user:password@prod-db:5432/wetechfounders_prod
REDIS_URL=redis://prod-redis:6379/0

# Feature Flags
ENABLE_DEBUG_LOGGING=false
ENABLE_DEVELOPMENT_MODE=false
ENABLE_STRICT_VALIDATION=true
ENABLE_ERROR_REPORTING=true
ENABLE_MONITORING=true

# Security
JWT_SECRET={{PRODUCTION_JWT_SECRET}}
API_KEY={{PRODUCTION_API_KEY}}

# External Services
STRIPE_PUBLIC_KEY={{PRODUCTION_STRIPE_PUBLIC_KEY}}
STRIPE_SECRET_KEY={{PRODUCTION_STRIPE_SECRET_KEY}}

# Logging
LOG_LEVEL=warn
LOG_FORMAT=json

# Performance
CACHE_TTL=3600
RATE_LIMIT=10000