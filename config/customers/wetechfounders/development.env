# Customer Development Environment Variables
# Template for wetechfounders development environment

# Customer Information
CUSTOMER_NAME=wetechfounders
CUSTOMER_DOMAIN=wetechfounders.com
ENVIRONMENT=development

# Domain Configuration
DOMAIN=dev.wetechfounders.com
API_DOMAIN=api-dev.dev.wetechfounders.com
AUTH_DOMAIN=auth-dev.dev.wetechfounders.com

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/wetechfounders_dev
REDIS_URL=redis://localhost:6379/0

# Feature Flags
ENABLE_DEBUG_LOGGING=true
ENABLE_DEVELOPMENT_MODE=true
ENABLE_STRICT_VALIDATION=false

# Security
JWT_SECRET=dev-jwt-secret-wetechfounders
API_KEY=dev-api-key-wetechfounders

# External Services
STRIPE_PUBLIC_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...

# Logging
LOG_LEVEL=debug
LOG_FORMAT=json

# Performance
CACHE_TTL=300
RATE_LIMIT=1000