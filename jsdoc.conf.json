{
  "source": {
    "include": ["./src/", "./types/"],
    "exclude": ["node_modules/", "dist/", "test/"]
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}